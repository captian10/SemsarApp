{
  "expo": {
    "name": "سمسار",
    "slug": "semsarapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "semsarapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#000000"
    },

    "androidStatusBar": {
      "translucent": true,
      "backgroundColor": "#000000"
    },
    "androidNavigationBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.captian10.semsarapp",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "We use this data to provide a better experience and improve the app.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.captian10.semsarapp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-notifications",
      "expo-font",
      [
        "expo-navigation-bar",
        {
          "position": "absolute",
          "backgroundColor": "#000000",
          "barStyle": "light",
          "borderColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8cd5e950-1809-4416-919e-cfc88727c29d"
      }
    },
    "owner": "captian10"
  }
}
